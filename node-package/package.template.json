{
  "name": "katod",
  "version": "__PACKAGE_VERSION__",
  "description": "Converter definition files to Kotlin declarations. A fork of Dukat project. Currently support TypeScript declarations",
  "bin": {
    "katod": "./bin/katod-cli.js"
  },
  "files": [
    "build/runtime/dukat-cli.jar",
    "bin/katod-cli.js",
    "lib/converter.js",
    "resources/stdlib.dukat",
    "d.ts.libs/*",
    "kotlin-stdlib-js/*.meta.js",
    "README.md"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nord-Schwabing-SL/katod.git"
  },
  "keywords": [
    "Kotlin",
    "Language",
    "TypeScript",
    "JavaScript",
    "DefinitelyTyped"
  ],
  "author": "Schwabing Nord Softwarel√∂sungen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Kotlin/katod/issues"
  },
  "homepage": "https://github.com/Nord-Schwabing-SL/katod/blob/main/README.md",
  "dependencies": {
    "typescript": "__TS_VERSION__",
    "google-protobuf": "__PROTOBUF_VERSION__"
  }
}
